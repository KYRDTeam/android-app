<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context="com.kyberswap.android.presentation.main.profile.ProfileFragment">

    <FrameLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/screen_background">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:text="@string/setting"
            android:textColor="#ffffff"
            android:textSize="18sp" />

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="58dp"
            android:fillViewport="true">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/color_bg_gray"
                android:paddingStart="24dp"
                android:paddingEnd="24dp">

                <ImageView
                    android:id="@+id/imgManageWallet"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginTop="24dp"
                    android:src="@drawable/ic_manage_wallet"
                    app:layout_constraintEnd_toStartOf="@+id/tvManageWallet"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintHorizontal_chainStyle="packed"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/tvManageWallet"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="24dp"
                    android:lineSpacingExtra="10sp"
                    android:text="@string/manage_walllet"
                    android:textColor="#141927"
                    android:textSize="14sp"
                    app:layout_constraintBottom_toBottomOf="@+id/imgManageWallet"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/imgManageWallet"
                    app:layout_constraintTop_toTopOf="@+id/imgManageWallet" />

                <ImageView
                    android:id="@+id/imgManageAlert"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginTop="18dp"
                    android:src="@drawable/ic_manage_alert"
                    app:layout_constraintEnd_toStartOf="@+id/tvManageAlert"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintHorizontal_chainStyle="packed"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imgManageWallet" />

                <TextView
                    android:id="@+id/tvManageAlert"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="24dp"
                    android:lineSpacingExtra="10sp"
                    android:text="@string/manage_alert"
                    android:textColor="#141927"
                    android:textSize="14sp"
                    app:layout_constraintBottom_toBottomOf="@+id/imgManageAlert"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/imgManageAlert"
                    app:layout_constraintTop_toTopOf="@+id/imgManageAlert" />

                <ImageView
                    android:id="@+id/imgAlertMethod"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginTop="18dp"
                    android:src="@drawable/ic_alert_method"
                    app:layout_constraintEnd_toStartOf="@+id/tvAlertMethod"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintHorizontal_chainStyle="packed"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imgManageAlert" />

                <TextView
                    android:id="@+id/tvAlertMethod"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="24dp"
                    android:lineSpacingExtra="10sp"
                    android:text="@string/alert_method"
                    android:textColor="#141927"
                    android:textSize="14sp"
                    app:layout_constraintBottom_toBottomOf="@+id/imgAlertMethod"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/imgAlertMethod" />

                <ImageView
                    android:id="@+id/imgContact"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginTop="18dp"
                    android:src="@drawable/ic_contacts"
                    app:layout_constraintEnd_toStartOf="@+id/tvContact"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintHorizontal_chainStyle="packed"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imgAlertMethod" />

                <TextView
                    android:id="@+id/tvContact"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="24dp"
                    android:lineSpacingExtra="10sp"
                    android:text="@string/setting_contact"
                    android:textColor="#141927"
                    android:textSize="14sp"
                    app:layout_constraintBottom_toBottomOf="@+id/imgContact"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/imgContact"
                    app:layout_constraintTop_toTopOf="@+id/imgContact" />

                <ImageView
                    android:id="@+id/imgSupport"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginTop="18dp"
                    android:src="@drawable/ic_support"
                    app:layout_constraintEnd_toStartOf="@+id/tvSupport"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintHorizontal_chainStyle="packed"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imgContact" />

                <TextView
                    android:id="@+id/tvSupport"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="24dp"
                    android:lineSpacingExtra="10sp"
                    android:text="@string/setting_support"
                    android:textColor="#141927"
                    android:textSize="14sp"
                    app:layout_constraintBottom_toBottomOf="@+id/imgSupport"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/imgSupport"
                    app:layout_constraintTop_toTopOf="@+id/imgSupport" />

                <ImageView
                    android:id="@+id/imgChangePin"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginTop="18dp"
                    android:src="@drawable/ic_change_pin"
                    app:layout_constraintEnd_toStartOf="@+id/tvChangePin"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintHorizontal_chainStyle="packed"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imgSupport" />

                <TextView
                    android:id="@+id/tvChangePin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="24dp"
                    android:lineSpacingExtra="10sp"
                    android:text="@string/change_pin"
                    android:textColor="#141927"
                    android:textSize="14sp"
                    app:layout_constraintBottom_toBottomOf="@+id/imgChangePin"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/imgChangePin"
                    app:layout_constraintTop_toTopOf="@+id/imgChangePin" />

                <ImageView
                    android:id="@+id/imgAbout"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginTop="18dp"
                    android:src="@drawable/ic_about"
                    app:layout_constraintEnd_toStartOf="@+id/tvAbout"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintHorizontal_chainStyle="packed"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imgChangePin" />

                <TextView
                    android:id="@+id/tvAbout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="24dp"
                    android:lineSpacingExtra="10sp"
                    android:text="@string/setting_about"
                    android:textColor="#141927"
                    android:textSize="14sp"
                    app:layout_constraintBottom_toBottomOf="@+id/imgAbout"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/imgAbout"
                    app:layout_constraintTop_toTopOf="@+id/imgAbout" />

                <ImageView
                    android:id="@+id/imgCommunity"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginTop="18dp"
                    android:src="@drawable/ic_language"
                    app:layout_constraintEnd_toStartOf="@+id/tvCommunity"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintHorizontal_chainStyle="packed"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imgAbout" />

                <TextView
                    android:id="@+id/tvCommunity"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="24dp"
                    android:lineSpacingExtra="10sp"
                    android:text="@string/setting_community"
                    android:textColor="#141927"
                    android:textSize="14sp"
                    app:layout_constraintBottom_toBottomOf="@+id/imgCommunity"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/imgCommunity"
                    app:layout_constraintTop_toTopOf="@+id/imgCommunity" />

                <ImageView
                    android:id="@+id/imgTelegram"
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:layout_marginStart="44dp"
                    android:src="@drawable/ic_telegram"
                    app:layout_constraintBottom_toBottomOf="@+id/imgTelegramDeveloper"
                    app:layout_constraintEnd_toStartOf="@+id/imgTelegramDeveloper"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintHorizontal_chainStyle="spread_inside"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="@+id/imgTelegramDeveloper" />

                <ImageView
                    android:id="@+id/imgTelegramDeveloper"
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:src="@drawable/ic_telegram_developer"
                    app:layout_constraintBottom_toBottomOf="@+id/imgGithub"
                    app:layout_constraintEnd_toStartOf="@+id/imgGithub"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/imgTelegram"
                    app:layout_constraintTop_toTopOf="@+id/imgGithub" />

                <ImageView
                    android:id="@+id/imgGithub"
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:src="@drawable/ic_github"
                    app:layout_constraintBottom_toBottomOf="@+id/imgFacebook"
                    app:layout_constraintEnd_toStartOf="@+id/imgFacebook"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/imgTelegramDeveloper"
                    app:layout_constraintTop_toTopOf="@+id/imgFacebook" />

                <ImageView
                    android:id="@+id/imgFacebook"
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:src="@drawable/ic_facebook"
                    app:layout_constraintBottom_toBottomOf="@+id/imgTwitter"
                    app:layout_constraintEnd_toStartOf="@+id/imgTwitter"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/imgGithub"
                    app:layout_constraintTop_toTopOf="@+id/imgTwitter" />

                <ImageView
                    android:id="@+id/imgTwitter"
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:src="@drawable/ic_twitter"
                    app:layout_constraintBottom_toBottomOf="@+id/imgMedium"
                    app:layout_constraintEnd_toStartOf="@+id/imgMedium"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/imgFacebook"
                    app:layout_constraintTop_toTopOf="@+id/imgMedium" />

                <ImageView
                    android:id="@+id/imgMedium"
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:src="@drawable/ic_medium"
                    app:layout_constraintBottom_toBottomOf="@+id/imgLinkedin"
                    app:layout_constraintEnd_toStartOf="@+id/imgLinkedin"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/imgTwitter"
                    app:layout_constraintTop_toTopOf="@+id/imgLinkedin" />

                <ImageView
                    android:id="@+id/imgLinkedin"
                    android:layout_width="32dp"
                    android:layout_height="32dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="16dp"
                    android:src="@drawable/ic_linked_in"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/imgMedium"
                    app:layout_constraintTop_toBottomOf="@+id/imgCommunity" />

                <ImageView
                    android:id="@+id/imgShare"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginTop="18dp"
                    android:src="@drawable/ic_share"
                    app:layout_constraintEnd_toStartOf="@+id/tvShare"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintHorizontal_chainStyle="packed"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imgTelegram" />

                <TextView
                    android:id="@+id/tvShare"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="24dp"
                    android:lineSpacingExtra="10sp"
                    android:text="@string/share_with_friends"
                    android:textColor="#141927"
                    android:textSize="14sp"
                    app:layout_constraintBottom_toBottomOf="@+id/imgShare"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/imgShare"
                    app:layout_constraintTop_toTopOf="@+id/imgShare" />

                <ImageView
                    android:id="@+id/imgFavorite"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginTop="18dp"
                    android:src="@drawable/ic_favorite"
                    app:layout_constraintEnd_toStartOf="@+id/tvFavorite"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintHorizontal_chainStyle="packed"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imgShare" />

                <TextView
                    android:id="@+id/tvFavorite"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="24dp"
                    android:lineSpacingExtra="10sp"
                    android:text="@string/rate_app"
                    android:textColor="#141927"
                    android:textSize="14sp"
                    app:layout_constraintBottom_toBottomOf="@+id/imgFavorite"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toEndOf="@+id/imgFavorite"
                    app:layout_constraintTop_toTopOf="@+id/imgFavorite" />

                <TextView
                    android:id="@+id/tvVersion"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12dp"
                    android:lineSpacingExtra="14sp"
                    android:text="Version 1.1.0 - 18 - Production"
                    android:textColor="#5a5e67"
                    android:textSize="10sp"
                    app:layout_constraintStart_toStartOf="@+id/tvFavorite"
                    app:layout_constraintTop_toBottomOf="@+id/tvFavorite"
                    tools:ignore="SmallSp" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.core.widget.NestedScrollView>


    </FrameLayout>


</layout>


